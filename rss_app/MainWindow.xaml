<Window x:Class="rss_app.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:rss_app"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="password" TextWrapping="Wrap" Text="Password" Grid.Column="2" Grid.Row="2" Background="Red"/>
        <TextBox x:Name="username" TextWrapping="Wrap" Text="Username" Grid.Column="1" Grid.Row="2"  Background="Red"/>
        <Button x:Name="button_login" Click="button_login_Click" Content="Login" HorizontalAlignment="Left" Grid.Row="3" Grid.Column="2" VerticalAlignment="Top"/>

    </Grid>
</Window>
